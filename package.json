{
  "name": "betahub-webform",
  "version": "0.0.4",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "watch": "microbundle -o dist/bhwf.min.js --format iife,es watch & npm run watch-css",
    "build": "microbundle -o dist/bhwf.min.js --format iife,es && npm run minify-css",
    "prewatch": "mkdir -p dist",
    "watch-css": "nodemon -w src -e css --exec 'npm run minify-css'",
    "minify-css": "for file in src/*.css; do cleancss -o dist/$(basename $file .css).min.css $file; done"
  },
  "source": "src/index.ts",
  "keywords": [
    "betahub",
    "webform"
  ],
  "author": "b3nr0w",
  "license": "MIT",
  "description": "BetaHub Webform",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dropzone": "^5.7.8",
    "clean-css-cli": "^5.6.3",
    "dropzone": "^6.0.0-beta.2",
    "microbundle": "^0.15.1",
    "nodemon": "^3.1.7",
    "typescript": "^5.6.3"
  }
}
