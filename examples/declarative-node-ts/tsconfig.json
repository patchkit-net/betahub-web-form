{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "target": "ES6",
    "moduleResolution": "Node10",
    "module": "ES6",
    "declaration": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true
  }
}
